{{ form_start(form) }}
<ul id="lol"
        data-prototype="{{ form_widget(form.images.vars.prototype)|e }}"
        data-widget-tags="{{ '<li></li>'|e }}"
        data-widget-counter="{{ form.images|length }}">
        {% for imageField in form.images %}
        <li>
            {{ form_errors(imageField) }}
            {{ form_widget(imageField) }}
        </li>
        {% endfor %}
    </ul>
    <button type="button"
    class="add-another-collection-widget"
    data-list-selector="#lol">Add another image</button>

{# ... #}
  
    <input type="submit" name="" value="{{ submitButtonText ?? 'Enregister' }}" formnovalidate>
{{ form_end(form) }}
 
<script src="/js/add-collection-widget.js"></script>